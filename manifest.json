{
  "manifest_version": 3,
  "name": "Netflix Aspect Ratio Fixer",
  "description": "Change your Netflix video aspect ratio to 16:9",
  "version": "1.0",
  "icons": {
    "16": "images/netflixOnDesk_16.png",
    "32": "images/netflixOnDesk_32.png",
    "48": "images/netflixOnDesk_48.png",
    "128": "images/netflixOnDesk_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*"],
      "js": ["content.js"]
    }
  ]
}
